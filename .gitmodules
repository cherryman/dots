[submodule "zsh/.zplug"]
	path = zsh/zplug
	url = https://github.com/zplug/zplug.git
[submodule "vim/.vim/vim-plug"]
	path = vim/vim-plug
	url = https://github.com/junegunn/vim-plug.git
[submodule "zsh/.config/base16-shell"]
	path = base16-shell
	url = https://github.com/chriskempson/base16-shell.git
[submodule "tmux/.tmux/plugins/tpm"]
	path = tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "emacs"]
	path = emacs
	url = https://github.com/doomemacs/doomemacs
