snippet pow binary exponentiation
	ll bpow(ll a, ll b, ll m) {
		a %= m;
		ll r = 1;
		while (b) {
			if (b & 1) r = r * a % m;
			a = a * a % m;
			b >>= 1;
		}
		return r;
	}
