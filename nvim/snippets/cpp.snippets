snippet manacher manacher's
	template <typename T>
	void manacher(const string &s, T &d, bool even) {
		for (int i = 0, l = 0, r = -1; i < s.size(); ++i) {
			int k = (i > r) ? !even : min(d[l+r-i+even], r-i+1);
			while (i-k-even >= 0 && i+k < s.size() && s[i-k-even] == s[i+k]) ++k;
			d[i] = k--;
			if (i+k > r) {
				l = i - k - even;
				r = i + k;
			}
		}
	}
